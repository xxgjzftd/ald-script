{
  "name": "xxgjzftd-tool",
  "version": "1.0.17",
  "description": "",
  "main": "./lib/index.js",
  "bin": {
    "ald": "./lib/index.js"
  },
  "build": {
    "files": [
      "!wwwroot"
    ],
    "publish": [
      {
        "provider": "generic",
        "url": "http://127.0.0.1:8080/"
      }
    ],
    "appId": "xxgjzftd.tool",
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "electronDist": "electron-dist"
  },
  "scripts": {
    "transpile": "tsc",
    "start": "tsc & electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "publish": "npm run transpile & npm run dist & node publish.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xxgjzftd/ald-script.git"
  },
  "keywords": [],
  "author": "xxgjzftd <xxgjzftd@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/xxgjzftd/ald-script/issues"
  },
  "homepage": "https://github.com/xxgjzftd/ald-script#readme",
  "dependencies": {
    "dm.dll": "^1.0.8",
    "electron-log": "^3.0.5",
    "electron-updater": "^4.0.6",
    "fs-extra": "^7.0.1"
  },
  "devDependencies": {
    "@types/minimist": "^1.2.0",
    "@types/node": "^11.9.6",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "http-server": "^0.11.1",
    "tslint": "^5.13.1",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.3.3333"
  }
}
